<script>
  const nav_header = document.querySelector(".header-area")
  const main_header = document.querySelector(".main-header");


  const observer = new IntersectionObserver(
    (entries) => {
      const ent = entries[0];
      console.log(ent);
      ent.isIntersecting == false
        ? document.body.classList.add("header-sticky")
        : document.body.classList.remove("header-sticky");

    },
    {

      root: null,
      rootMargin: "-50px",
      threshold: 0,

    });

  observer.observe(main_header);

  const form = document.getElementById('subscription-form');
  const warningMsg = document.getElementById('warning-msg');

  form.addEventListener('submit', function (event) {
    event.preventDefault(); // Prevent the default form submission

    // Get the email value from the input field
    const emailInput = form.querySelector('input[name="EMAIL"]');
    const email = emailInput.value;

    // Perform validation on the email address
    if (email.trim() === '') {
      // Display warning message if the input is empty
      warningMsg.textContent = 'Please enter an email address.';
      return; // Exit the function and prevent further execution
    } else {
      // Clear any previous warning message
      warningMsg.textContent = '';
    }

    // Submit the form data to the server using AJAX or fetch
    // Here's an example using fetch:
    fetch('/subscribe', {
      method: 'POST',
      headers: {
        'Content-Type': 'application/json'
      },
      body: JSON.stringify({ email: email })
    })
      .then(response => {
        // Handle the server response
        if (response.ok) {
          // Subscription successful
          alert('Thank you for subscribing!');
        } else {
          // Subscription failed
          alert('Subscription failed. Please try again.');
        }
      })
      .catch(error => {
        // Handle any errors
        console.error('Error:', error);
        alert('An error occurred. Please try again later.');
      });
  });
</script>

