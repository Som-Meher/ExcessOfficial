  const observer = new IntersectionObserver(
    (entries) => {
      const ent = entries[0];
      console.log(ent);
      ent.isIntersecting == false
        ? document.body.classList.add("header-sticky")
        : document.body.classList.remove("header-sticky");
    },
    {
      root: null,
      rootMargin: "-50px",
      threshold: 0,
    }
  );

  observer.observe(main_header);
</script>

