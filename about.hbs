<script>
  function clock() {
    var date = new Date();
    var hours = date.getHours();
    var minutes = date.getMinutes();
    var seconds = date.getSeconds();
    var midday;

    hours = updateTime(hours);
    minutes = updateTime(minutes);
    seconds = updateTime(seconds);

    //If else condition
    midday = hours >= 12 ? "PM" : "AM";
    document.getElementById("clock").innerHTML =
      hours + ":" + minutes + ":" + seconds + midday;

    var time = setTimeout(function () {
      clock();
    }, 1000);

    //Morning Afternoon and Evening condition
    if (hours < 12) {
      var greeting = "Goodmorning" + "HurryUp!";
    }
    if (hours >= 12 && hours <= 18) {
      var greeting = "Goodafternoon";
    }
    if (hours >= 18 && hours <= 24) {
      var greeting = "Goodevening";
    }
    document.getElementById("greeting").innerHTML = greeting;
  }
  function updateTime(k) {
    if (k < 10) {
      return "0" + k;
    } else {
      return k;
    }
  }
  clock();

  /*-----------------------------------------------------------------*/

  const nav_header = document.querySelector(".header-area");
